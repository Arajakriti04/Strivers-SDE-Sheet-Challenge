#include <bits/stdc++.h> 
bool areAnagram(string &str1, string &str2){
    // Write your code here.
    int n=str1.size();
    int m=str2.size();
    if(n!=m)
    return 0;
    sort(str1.begin(),str1.end());
    sort(str2.begin(),str2.end());
    if(str1==str2)
    return 1;
    else
    return 0;
}
